{
	"name": "Selenized (0.0.2)",
	"type": "dark",
	"colors": {
		"editor.background":                   "#1e1e1e",
		"editor.foreground":                   "#c1c1c1",
		"editorCursor.foreground":             "#c1c1c1",
		"editorLineNumber.foreground":         "#7e7e7e",
		"editorLineNumber.activeForeground":   "#c1c1c1",
		"editorIndentGuide.background":        "#414141",
		"editorIndentGuide.activeBackground":  "#7e7e7e",
		"editor.selectionBackground":          "#414141",
		"editor.inactiveSelectionBackground":  "#2b2b2b",
		"editor.selectionHighlightBackground": "#1d1d1d",
		"editor.selectionHighlightBorder":     "#414141"
	},
	"tokenColors": [
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f3504b"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7e7e7e"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#75b938"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#eb8447"
			}
		},
		{
			"scope": "constant.character",
			"settings": {
				"foreground": "#44cabb"
			}
		},
		{
			"name": "special constants like true, false, null",
			"scope": "constant.language",
			"settings": {
				"foreground": "#eb8447"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e0b732"
			}
		},
		{
			"name": "if, else, return",
			"scope": "keyword.control",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e0b732"
			}
		},
		{
			"scope": "keyword.control.import",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f073bc"
			}
		},
		{
			"name": "def, class etc",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e0b732"
			}
		},
		{
			"name": "identifiers for fun/var declarations",
			"scope": "entity.name",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "things like const, final, abstract",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#44cabb"
			}
		},
		{
			"name": "types, casts etc.",
			"scope": "support.type",
			"settings": {
				"foreground": "#44cabb"
			}
		},
		{
			"name": "reserved language variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "italic"
			}
		}
	]
}

